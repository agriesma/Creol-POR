< "SantaClausC" : Cl | Inh: noInh, Par: noAid, Att: "ctRreindeers" |-> null , "waitRreindeers" |-> null , "harnessedReindeers" |-> null , "ctElves" |-> null , "waitElves" |-> null , "inofficeElves" |-> null, Mtds: 
  < "run" : Mtdname | Param: noAid, Latt: noSubst, Code: ( ".anon" ! "this" . "loop" (emp ) )  ; free( ".anon" ) ; return ( emp ) > *
  < "loop" : Mtdname | Param: noAid, Latt: noSubst, Code: ( ( await "=" ( "ctReindeers" # int(5) ) ) ; ( "sync.1" ! "this" . "deliverToys" (emp ) )  ; ( "sync.1" ? ( noAid ) )  [] ( await "&&" ( "<=" ( int(3) # "ctElves" ) # "~" ( "=" ( "ctReindeers" # int(9) ) ) ) ) ; ( "sync.0" ! "this" . "talkToElves" (emp ) )  ; ( "sync.0" ? ( noAid ) )  ) ; ( ".anon" ! "this" . "loop" (emp ) )  ; free( ".anon" ) ; return ( emp ) > *
