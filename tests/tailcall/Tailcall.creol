interface I
begin
  with Any
    op m
    op p(out x: Int)
end

class C(i: I)
begin
  op m == i.m(;)
  op n == var l: Label [ ]; l!i.m(); l?()
  op p(out x: Int) == i.p(;x)
  op p(out x: Int) == var l: Label[Int]; l!i.p(); l?(x)
end
