reduce in PROGRAM-CHECKER : init .
result Configuration: 
< "Butler" : Cl | Inh: noInh, Par: noVid, Att: "p1" |-> null, "p2" |-> null,
    "p3" |-> null, Mtds:
  < "getNeighbor" : Mtdname | Param: noVid, Latt: "neighbor" |-> null, Code: if
    "="("caller" # "p1") th "neighbor" ::= "p2" el if "="("caller" # "p2") th
    "neighbor" ::= "p3" el "neighbor" ::= "p1" fi fi ; return("neighbor") > *
  < "init" : Mtdname | Param: noVid, Latt: noSubst, Code: skip ; return(emp) >
    *
  < "run" : Mtdname | Param: noVid, Latt: noSubst, Code: "p1" ::= new
    "Philosopher"("this") ; "p2" ::= new "Philosopher"("this") ; "p3" ::= new
    "Philosopher"("this") ; return(emp) >,
  Ocnt: 0 >

< "Philosopher" : Cl | Inh: noInh, Par: "butler", Att: "butler" |-> null,
    "chopstick" |-> null, "hungry" |-> null, "neighbor" |-> null, Mtds:
  < "borrowStick" : Mtdname | Param: noVid, Latt: noSubst, Code: await
    "chopstick" ; "chopstick" ::= bool(false) ; return(emp) > *
  < "eat" : Mtdname | Param: noVid, Latt: "label:0" |-> null, "label:1" |->
    null, "label:2" |-> null, Code: await "hungry" ; "label:0" ! "neighbor" .
    "borrowStick"(emp) ; await ("label:0" ??) ; "label:0" ?(noVid) ; await
    "chopstick" ; "hungry" ::= bool(false) ; "label:1" ! "neighbor" .
    "returnStick"(emp) ; free("label:1") ; "label:1" ::= null ; "label:2" !
    "this" . "eat"(emp) ; free("label:2") ; "label:2" ::= null ; return(emp) >
    *
  < "init" : Mtdname | Param: noVid, Latt: "label:0" |-> null, Code:
    "chopstick" ::= bool(true) ; "hungry" ::= bool(false) ; "label:0" !
    "butler" . "getNeighbor"(emp) ; "label:0" ?("neighbor") ; return(emp) > *
  < "returnStick" : Mtdname | Param: noVid, Latt: noSubst, Code: "chopstick"
    ::= bool(true) ; return(emp) > *
  < "run" : Mtdname | Param: noVid, Latt: "label:0" |-> null, "label:1" |->
    null, Code: "label:0" ! "this" . "think"(emp) ; free("label:0") ; "label:0"
    ::= null ; "label:1" ! "this" . "eat"(emp) ; free("label:1") ; "label:1"
    ::= null ; return(emp) > *
  < "think" : Mtdname | Param: noVid, Latt: "label:0" |-> null, Code: await
    "~"("hungry") ; release ; "hungry" ::= bool(true) ; "label:0" ! "this" .
    "think"(emp) ; free("label:0") ; "label:0" ::= null ; return(emp) >,
  Ocnt: 0 >
