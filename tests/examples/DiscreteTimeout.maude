< "DiscreteTimeout" : Cl | Inh: (noInh),
  Par: ("other"),
  Att: ("other" |-> null , "clock" |-> null),
  Mtds: (< "init" : Mtdname | Param: noVid, Latt: "_" |-> null, Code: ( "clock" @@ "DiscreteTimeout" ) ::= int(0) ; return ( emp ) > *
