    < "timeout" : Mtdname | Param: "n", Latt: "n" |-> null , "result" |-> null , "timeout" |-> null , "l" |-> null , "_" |-> null, Code: "timeout" ::= "+" ( "clock" :: int(10) ) ; "l" ! "other" . "m" ( emp ) ; ( ( await "&&" ( ( "l" ?? ) :: "<=" ( "clock" :: "timeout" ) ) ) ; ( "l" ? ( noVid ) )  ; "result" ::= bool(true) [] ( await ">" ( "clock" :: "timeout" ) ) ; free( "l" ) ; "result" ::= bool(false) ) ; return ( "result" ) >),
  Ocnt: 0 >

.
endm
