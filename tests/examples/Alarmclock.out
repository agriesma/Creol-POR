reduce in PROGRAM : init .
result Class: 
< "Alarmclock" : Cl | Inh: noInh, Par: noVid, Att: "clock" |-> null, Mtds:
  < "init" : Mtdname | Param: noVid, Latt: noSubst, Code: "clock" ::= int(0) ;
    return(emp) > *
  < "run" : Mtdname | Param: noVid, Latt: noSubst, Code: "clock" ::= "+"(
    "clock" # int(1)) ; ".anon" ! "this" . "run"(emp) ; free(".anon") ; return(
    emp) > *
  < "wakeme" : Mtdname | Param: "n", Latt: "alarmsetting" |-> null, "n" |->
    null, Code: "alarmsetting" ::= "+"("clock" # "n") ; await ">="("clock" #
    "alarmsetting") ; return(emp) >,
  Ocnt: 0 >
Bye.
