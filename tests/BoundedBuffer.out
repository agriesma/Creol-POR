< "BoundedBuffer" : Cl | Inh: noInh, Par: noAid, Att: "buffer" |-> null, "max"
    |-> null, "n" |-> null, Mtds:
  < "append" : Mtdname | Param: "d", Latt: "d" |-> null, Code: await "<"("n" #
    "max") ; "buffer" ::= "plus"("buffer" # "d") ; return(emp) > *
  < "remove" : Mtdname | Param: noAid, Latt: "d" |-> null, Code: await ">"("n"
    # int(0)) ; "d" ::= "first"("buffer") ; "buffer" ::= "rest"("buffer") ;
    return("d") >,
  Ocnt: 0 >

< "Cons" : Cl | Inh: noInh, Par: "b", Att: "b" |-> null, Mtds:
  < "loop" : Mtdname | Param: noAid, Latt: "y" |-> null, Code: "sync.4" ! "b" .
    "remove"(emp) ; "sync.4" ?("y") ; "anon.5" ! "this" . "loop"(emp) ; return(
    emp) > *
  < "run" : Mtdname | Param: noAid, Latt: noSubst, Code: "anon.3" ! "this" .
    "loop"(emp) ; return(emp) >,
  Ocnt: 0 >

< "Prod" : Cl | Inh: noInh, Par: "b", Att: "b" |-> null, Mtds:
  < "loop" : Mtdname | Param: "i", Latt: "i" |-> null, Code: "sync.1" ! "b" .
    "append"("i") ; "sync.1" ?(noAid) ; "anon.2" ! "this" . "loop"("+"("i" #
    int(1))) ; return(emp) > *
  < "run" : Mtdname | Param: noAid, Latt: noSubst, Code: "anon.0" ! "this" .
    "loop"(int(0)) ; return(emp) >,
  Ocnt: 0 >

< "Starter" : Cl | Inh: noInh, Par: noAid, Att: noSubst, Mtds:
