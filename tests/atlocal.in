# Set the path to the uninstalled data files of the tool set
CREOL_PATH=@abs_top_srcdir@/share
export CREOL_PATH

CREOL_LIBRARY_PATH=@abs_top_srcdir@/share
export CREOL_LIBRARY_PATH

if test -z "$CREOLC"
then
  CREOLC=creolc
  export CREOLC
fi
 

# Modify the path of maude to find our interpreter definitions
MAUDE_LIB=$CREOL_PATH:@abs_top_srcdir@:$MAUDE_LIB
export MAUDE_LIB

if test -z "$MAUDE"
then
  MAUDE=@MAUDE@
  export MAUDE
fi
