# top source directory
abs_top_srcdir=@abs_top_srcdir@
export abs_top_srcdir

# Set the path to a good sed.
if test -z "$SED"
then
    SED=sed
    export SED
fi

# Set the path to the uninstalled data files of the tool set
CREOL_LIBRARY_PATH=@abs_top_srcdir@/share
export CREOL_LIBRARY_PATH

if test -z "$CREOLC"
then
  CREOLC=creolc
  export CREOLC
fi
 

# Modify the path of maude to find our interpreter definitions
if test -z "$MAUDE_LIB"
then
    MAUDE_LIB=@abs_top_srcdir@/share
    export MAUDE_LIB
else
    MAUDE_LIB=@abs_top_srcdir@/share:$MAUDE_LIB
    export MAUDE_LIB
fi

if test -z "$MAUDE"
then
  MAUDE=@MAUDE@
  export MAUDE
fi

WITH_METATESTS=@WITH_METATESTS@
